
SOLUTION1)

SELECT DISTINCT CITY FROM STATION WHERE SUBSTRING(CITY,1,1) IN ("A","E","I",'O','U')

SOLUTION2)
SELECT DISTINCT CITY FROM STATION
WHERE CITY LIKE "%a" or CITY LIKE "%e" or 
CITY LIKE "%i" or 
CITY LIKE "%o" or 
CITY LIKE "%u";

SOLUTION3)
SELECT DISTINCT CITY 
FROM STATION
WHERE CITY REGEXP '^[aeiouAEIOU]';
