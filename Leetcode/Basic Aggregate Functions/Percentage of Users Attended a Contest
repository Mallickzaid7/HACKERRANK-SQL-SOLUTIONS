SELECT CONTEST_ID AS contest_id, ROUND((INDI_USERS/A)*100,2) AS percentage
FROM
(SELECT R.CONTEST_ID AS CONTEST_ID, COUNT(R.USER_ID) AS INDI_USERS, 
(SELECT COUNT(USER_ID) AS TOTAL_USERS FROM USERS)  A
 FROM USERS U join REGISTER R
ON U.user_id=R.user_id
GROUP BY CONTEST_ID
ORDER BY CONTEST_ID) B
ORDER BY percentage desc, CONTEST_ID ASC;

